{
  "name": "@jlongyam/color",
  "version": "0.1.0",
  "description": "",
  "main": "./dist.color.cjs",
  "scripts": {
    "test": "node ./test/color.test.js && exit 1",
    "test:legacy": "~/Program/node-v5.12.0-darwin-x64/bin/node ./dist/color.test.js",
    "buble": "buble src -o stage -n moduleImport,moduleExport",
    "bundle": "rollup -c",
    "babel": "babel stage -d dist && rm -rf stage",
    "server": "serve ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/color.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/jlongyam/color/issues"
  },
  "homepage": "https://github.com/jlongyam/color#readme",
  "dependencies": {
    "@jlongyam/env": "^1.1.3"
  },
  "engines": {
    "node": ">=5.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "buble": "^0.20.0",
    "rollup": "^4.49.0",
    "serve": "^14.2.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-modules-commonjs",
        {
          "allowTopLevelThis": true,
          "importInterop": "node",
          "strict": true,
          "strictMode": false
        }
      ]
    ]
  }
}
